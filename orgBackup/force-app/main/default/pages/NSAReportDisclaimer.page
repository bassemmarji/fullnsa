    <head>
        <style type="text/css" media="print">
            @page{
                size: A4 landscape;
            } 

            div.header {
                padding: 10px;
                position: running(header);
            }

            div.footers {

                
            }
            #head{
height:10%;
}
            #boxes1{
                height: 20%;
                font-size: small; font-family: 'Helvetica', 'Arial', sans-serif;
                margin-top: 5%;


            }
            #boxes2{
                height: 25%;
                font-size: small; font-family: 'Helvetica', 'Arial', sans-serif;
                width: 100%;
                margin-top: 5%;

            }
            #tableBig1{
				float:left; 
                width:100%;
				height: 35%
               
			}
			#tableBig2{
				float:left; 
                height:15%;
               
			}
  #leftbox {
                float:left; 
                width:28%;

            }
            #leftbox2 {
                float:left; 
                width:40%;

            }
            #middlebox2{
                float:right; 
                width:40%;
                margin-left: 5%;

            }
            #middlebox{
                float:left; 
                width:33%;
                margin-left: 3%;

            }
            #rightbox{
                float:right;
                width:33%;
                margin-left: 3%

            }
            h1{
                text-align:center;
            }
			
            #smallTable,thead,tr,th{
border:none;
 }
 #smallTable{
    width: 100%;
 }
 #smallTable,tbody,tr,td{
border-top: 1px solid black;
border-right: 1px solid black;
}
 #smallTable,tbody,tr:nth-child(1){
border:none;
}
 #smallTable,tbody,tr,td:last-child{
border-right: none;
}

#title{
    align-content: center;
    width: 80%;
    height: 100%;
}
#logo{
    float:left;
    width: 20%;
    height: 100%;
}
#main{height: 70%;}
#ending{height: 30%;}

        </style>
