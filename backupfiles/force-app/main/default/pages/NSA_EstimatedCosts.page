<apex:page title="NSA Estimated Costs" renderAs="pdf" controller="NSA_AdvancedEOBController" standardStylesheets="false" applyBodyTag="false">
    <head>
        <style type="text/css" media="print">
            @page {
                size: A4 landscape;
            }

            div.header {
                padding: 10px;
                position: running(header);
            }

            div.footers {
            }
            #head {
                height: 10%;
            }
            #boxes1 {
                height: 100%;
                font-size: small;
                font-family: "Helvetica", "Arial", sans-serif;
                margin-top: 10%;
            }
            #boxes2 {
                height: 100%;
                font-size: small;
                font-family: "Helvetica", "Arial", sans-serif;
                width: 100%;
                margin-top: 5%;
            }
            #tableBig1 {
                float: left;
                width: 100%;
                height: 35%;
            }
            #tableBig2 {
                float: left;
                height: 15%;
            }
            #leftbox {
                float: left;
                width: 28%;
            }
            #leftbox2 {
                float: left;
                width: 35%;
                height: 100%;
            }
            #middlebox2 {
                float: right;
                width: 30%;
                height: 100%;
            }
            #middlebox {
                float: left;
                width: 33%;
                margin-left: 3%;
            }
            #rightbox {
                float: right;
                width: 33%;
                margin-left: 3%;
            }
            h1 {
                text-align: center;
            }

            #smallTable,
            thead,
            tr,
            th {
                border: none;
            }
            #smallTable {
                width: 100%;
            }
            #smallTable,
            tbody,
            tr,
            td {
                border-top: 1px solid black;
                border-right: 1px solid black;
            }
            #smallTable,
            tbody,
            tr:nth-child(1) {
                border: none;
            }
            #smallTable,
            tbody,
            tr,
            td:last-child {
                border-right: none;
            }

            #title {
                align-content: center;
                width: 80%;
                height: 100%;
            }
            #logo {
                float: left;
                width: 20%;
                height: 100%;
            }
            #main {
                height: 100%;
                width: 100%;
            }
            #ending {
                height: 30%;
            }
        </style>
    </head>

    <div id="main" style="{!IF( showPDF == false, 'display:none', '')}">
       
        <div style="page-break-after:always;">
            <div id="head">
                <div id="logo"><img style="height: 70%; width: 70%; " src="{!$Resource.IPSLogo}" /></div>
    
                <div id="title"><h2 style="text-align: center; padding-bottom: 5px;">{!$Label.NSA_Report_Title}</h2></div>
            </div>
            <div id="boxes1">
                <div id="leftbox">
                    <table id="smallTable">
                        <thead>
                            <tr>
                                <th>
                                    <b>Patient Details</b>
                                </th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>Member ID</td>
                                <td>
                                    <apex:outputText value="{!eob.IntPS_NSA__Patient__r.IntPS__Not_Unique_Member_Id__c}"> </apex:outputText>
                                </td>
                            </tr>
                            <tr>
                                <td>First Name Last Name</td>
                                <td>
                                    <apex:outputText value="{!eob.IntPS_NSA__Patient__r.IntPS__First_Name__c}  {!eob.IntPS_NSA__Patient__r.IntPS__Last_Name__c} " escape="false" />
                                </td>
                            </tr>
                            <tr>
                                <td>Address</td>
                                <td>
                                    <apex:outputText value="{!eob.IntPS_NSA__Patient__r.IntPS__Street__c} {!eob.IntPS_NSA__Patient__r.IntPS__Street1__c}" escape="false" />
                                </td>
                            </tr>
    
                            <tr>
                                <td>Address</td>
                                <td>
                                    <apex:outputText value="{!eob.IntPS_NSA__Patient__r.IntPS__City__c} {!eob.IntPS_NSA__Patient__r.IntPS__State__c} , {!eob.IntPS_NSA__Patient__r.IntPS__Zip_Code__c}" escape="false" />
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
    
                <div id="middlebox">
                    <table id="smallTable">
                        <thead>
                            <tr>
                                <th>
                                    <b>Health Plan Details</b>
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>Employer Group Name</td>
                                <td>
                                    <apex:outputText value="{!eob.IntPS_NSA__Patient__r.IntPS__Employer__r.Name}" escape="false" />
                                </td>
                            </tr>
                            <tr>
                                <td>Health Plan Name</td>
                                <td>
                                    <apex:outputText value="{!eob.IntPS_NSA__Eligibility__r.IntPS__Health_Plan__r.Name}" escape="false" />
                                </td>
                            </tr>
                            <tr>
                                <td>Health Plan Number</td>
                                <td>
                                    <apex:outputText value="{!eob.IntPS_NSA__Eligibility__r.IntPS__Health_Plan__r.IntPS__ExternalID__c}" escape="false" />
                                </td>
                            </tr>
    
                            <tr>
                                <td>Eligibility Period (Start and End Date)</td>
                                <td><apex:outputText value="From {!eob.IntPS_NSA__Eligibility__r.IntPS__Begin_Date__c} To {!eob.IntPS_NSA__Eligibility__r.IntPS__Term_Date__c}" escape="false" /></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
    
                <div id="rightbox">
                    <table id="smallTable">
                        <thead>
                            <tr>
                                <th>
                                    <b>Plan Benefits Balance (at time of request)</b>
                                </th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>Deductible: Individual</td>
                                <td>
                                    <apex:outputText value="${!eob.IntPS_NSA__Total_Deductible_Paid_Amount__c}" escape="false" />
                                </td>
                            </tr>
                            <tr>
                                <td>Deductible: Family</td>
                                <td>
                                    <apex:outputText value="${!eob.IntPS_NSA__Total_Deductible_Paid_Amount_by_Family__c}" escape="false" />
                                </td>
                            </tr>
                            <tr>
                                <td>Out-of-Pocket Max: Individual</td>
                                <td>
                                    <apex:outputText value="{! IF($Label.NSA_OrgName=='Cornerstone','Not Captured',ROUND(eob.IntPS_NSA__Total_Out_Of_Pocket_Amount__c, 2))}"> </apex:outputText>
                                </td>
                            </tr>
    
                            <tr>
                                <td>Out-of-Pocket Max: Family</td>
                                <td>
                                    <apex:outputText value="{! IF($Label.NSA_OrgName=='Cornerstone','Not Captured',ROUND(eob.IntPS_NSA__Total_Out_Of_Pocket_Amount_by_Family__c,2))}"> </apex:outputText>
                                 </td>
                            </tr>
                            <tr>
                                <td>As of</td>
                                <td>
                                    <apex:outputText value="{!healthPlan.IntPS__TPA_Accumulators__r[0].LastModifiedDate}" 
                                    rendered="{!healthPlan.IntPS__TPA_Accumulators__r.size>0}" escape="false" />                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
    
            <div id="boxes2">
                <div id="leftbox2">
                    <table id="smallTable">
                        <thead>
                            <tr>
                                <th>
                                    <b>Request Details</b>
                                </th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>Date of Service</td>
                                <td>
                                   <!-- <apex:outputText value="{0,date,yyyy.MM.dd}">
                                        <apex:param value="{!eob.IntPS_NSA__Service_Date__c}"/>
                                   </apex:outputText>-->
                                   <apex:outputText value="{!serviceDate}">
                                   </apex:outputText>
                                   
                                </td>
                            </tr>
                            <tr>
                                <td>Request #</td>
                                <td>
                                    <apex:outputText value="{!eob.Name}" escape="false" />
                                </td>
                            </tr>
                            <tr>
                                <td>Request Date / Time</td>
                                <td>
                                    <apex:outputField value="{!eob.CreatedDate}"  />
                                </td>
                            </tr>
    
                            <tr>
                                <td>Network Rate Applied</td>
                                <td><apex:outputText value="{!eob.IntPS_NSA__MRF_Network__c}" escape="false" /></td>
                            </tr>
                            <tr>
                                <td>As of</td>
                                <td>
                                    <apex:outputText value="{!eob.IntPS_NSA__MRF_Date__c}" escape="false" />
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
    
                <div id="middlebox2">
                    <table id="smallTable">
                        <thead>
                            <tr>
                                <th>
                                    <b>Provider Details</b>
                                </th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>Provider Name</td>
                                <td>
                                    <apex:outputText value="{!eob.IntPS_NSA__Provider_Name__c}" escape="false" />
                                </td>
                            </tr>
                            <tr>
                                <td>NPI Number</td>
                                <td>
                                    <apex:outputText value="{!eob.IntPS_NSA__Provider_NPI_Number__c}" escape="false" />
                                </td>
                            </tr>
                            <tr>
                                <td>Address</td>
                                <td>
                                    <apex:outputText value="{!eob.IntPS_NSA__Provider_Billing_Street__c}" escape="false" />
                                </td>
                            </tr>
    
                            <tr>
                                <td>Address</td>
                                <td><apex:outputText value="{!eob.IntPS_NSA__Provider_Billing_City__c} {!eob.IntPS_NSA__Provider_Billing_State__c} , {!eob.IntPS_NSA__Provider_Billing_Postal_Code__c}" escape="false" /></td>
                            </tr>
                            <tr>
                                <td>Provider Network</td>
                                <td><apex:outputText value="{!eob.IntPS_NSA__Provider_Network__c}" escape="false" /></td>
                            </tr>
                            <tr>
                                <td>Available Provider Networks</td>
                                <td>
                                    <apex:outputText value="{! IF(eob.IntPS_NSA__Provider_Network__c=='Out-Of-Network','',eob.Available_Networks_For_Provider__c)}"> </apex:outputText>
                                </td>
                            </tr>
                            
                        </tbody>
                    </table>
                </div>
            </div>    
        </div>

        <div id="tableBig1" style="font-size: small; font-family: 'Helvetica', 'Arial', sans-serif; padding-top: 15px;">
            <table style="table-layout: fixed; overflow: hidden; empty-cells: hidden;  width: 100%; border: 2px solid white;" border="0" cellspacing="3" class="tbl">
                <thead>
                    <tr>
                        <th colspan="6" rowspan="2" style="width: 35%; border: 2px solid black !important;">
                            <div>
                                <p style="border-bottom: 1px solid black; font-size: small; text-align: center;">
                                    <strong>Services Details</strong>
                                </p>
                                <div class="row" style="font-size: 12px;">
                                    <div class="column" style="font-size: 10px; float: left; width: 16.66%; text-align: center;">
                                        Service Code
                                    </div>
                                    <div class="column" style="font-size: 10px; float: left; width: 33.333%; text-align: center;">
                                        Service Description
                                    </div>
                                    <div class="column" style=" font-size: 10px; float: left; width: 16.66%; text-align: center;">
                                        Contracted Rate
                                    </div>
                                    <div class="column" style="float: left; font-size: 10px; width: 16.66%; text-align: center">
                                        Qty                                    </div>
                                    <div class="column" style="font-size: 10px; float: left; width: 16.66%; text-align: center;">
                                        Total Contracted Costs
                                    </div>
                                </div>
                            </div>
                        </th>
                       
                        <th colspan="6" rowspan="2" style="width: 30%; border: 2px solid black !important;">
                            <div>
                                <p style="border-bottom: 1px solid black; font-size: small; text-align: center;">
                                    <strong>Patient Responsibility</strong>
                                </p>
                                <div class="row" style="font-size: 12px;">
                                    <div class="column" style="font-size: 10px; float: left; width: 16.666%; text-align: center;">
                                        Copay
                                    </div>
                                    <div class="column" style="font-size: 10px; float: left; width: 16.666%; text-align: center;">
                                        Coins.
                                    </div>
                                    <div class="column" style="font-size: 10px; float: left; width: 16.666%; text-align: center;">
                                        Ded.
                                    </div>
                                    <div class="column" style="float: left; font-size: 10px; width: 16.666%; text-align: center">
                                        Non-Covered
                                    </div>
                                    <div class="column" style="font-size: 10px; float: left; width: 16.666%; text-align: center;">
                                        Total estimated Costs
                                    </div>
                                    <div class="column" style="font-size: 10px; float: left; width: 16.666%; text-align: center;">
                                        Notation
                                    </div>
                                </div>
                            </div>
                        </th>
                        <th colspan="3" rowspan="2" style="width: 35%; border: 2px solid black !important;">
                            <div>
                                <p style="border-bottom: 1px solid black; font-size: small; text-align: center;">
                                    <strong>Pricing Request Questions</strong>
                                </p>
                                <div class="row" style="font-size: 12px;">
                                    <div class="column" style="font-size: 10px; float: left; width: 33.3%; text-align: center;">
                                        Type of Provider
                                    </div>
                                    <div class="column" style="font-size: 10px; float: left; width: 33.3%; text-align: center;">
                                        Preventative or Pregnancy Services
                                    </div>
                                    <div class="column" style="font-size: 10px; float: left; width: 33.3%; text-align: center;">
                                        Place of Service
                                    </div>
                                </div>
                            </div>
                        </th>
                    </tr>
                </thead>
                <tbody>
                    <apex:repeat var="lineItems" value="{!eob.IntPS__EOB_Line_Items__r}">
                        <tr style="border-top: 2px solid black;">
                            <td class="slds-cell-wrap" style="padding-left: 5px; text-align: center;">
                                <apex:outputText value="{!lineItems.IntPS__L2110_Procedure_Code__c}"> </apex:outputText>
                            </td>
                            <td colspan="2" class="slds-cell-wrap" style="padding-left: 5px; text-align: center;">
                                <apex:outputText value="{!lineItems.IntPS__L2110_Procedure_Code_Description__c}"> </apex:outputText>
                            </td>
                            <td style="text-align: right; padding-left: 5px; text-align: center;" class="slds-cell-wrap">
                                <apex:outputText value="{! IF(lineItems.IntPS_NSA__Service_Line_Allowed_Unit_Price__c==null,'No Rate',ROUND(lineItems.IntPS_NSA__Service_Line_Allowed_Unit_Price__c,2))}">
                                </apex:outputText>
                            </td>
                            <td style="text-align: right; padding-left: 5px; text-align: center;" class="slds-cell-wrap">
                                <apex:outputText value="{0,number,###,###,##0.00}" rendered="{! IF(ISNULL(lineItems.IntPS__L2110_Units_of_Service_Paid_Count__c),false,true)}">
                                    <apex:param value="{!lineItems.IntPS__L2110_Units_of_Service_Paid_Count__c}" />
                                </apex:outputText>
                            </td>
                            <td style="text-align: right; padding-left: 5px; text-align: center;" class="slds-cell-wrap">
                                <apex:outputText value="{! IF(lineItems.IntPS__L2110_Service_Line_Allowed_Amount__c==null,'No Rate',ROUND(lineItems.IntPS__L2110_Service_Line_Allowed_Amount__c,2))}">
                                </apex:outputText>
                            </td>
                            <td style="text-align: right; padding-left: 5px;" class="slds-cell-wrap">
                                <apex:outputText value="{0,number,###,###,##0.00}" rendered="{! IF(ISNULL(lineItems.IntPS_NSA__Copay_Amount__c),false,true)}">
                                    <apex:param value="{!lineItems.IntPS_NSA__Copay_Amount__c}" />
                                </apex:outputText>
                            </td>
                            <td style="text-align: right; padding-left: 5px; border-right: 0.5px solid black;" class="slds-cell-wrap">
                                <apex:outputText value="{0,number,###,###,##0.00}" rendered="{! IF(ISNULL(lineItems.IntPS_NSA__Coinsurance_Amount__c),false,true)}">
                                    <apex:param value="{!lineItems.IntPS_NSA__Coinsurance_Amount__c}" />
                                </apex:outputText>
                            </td>
                            <td style="text-align: right; padding-left: 5px;" class="slds-cell-wrap">
                                <apex:outputText value="{0,number,###,###,##0.00}" rendered="{! IF(ISNULL(lineItems.IntPS_NSA__Deductible_Amount__c),false,true)}">
                                    <apex:param value="{!lineItems.IntPS_NSA__Deductible_Amount__c}" />
                                </apex:outputText>
                            </td>
                            <td style="text-align: right; padding-left: 5px;" class="slds-cell-wrap">
                                <apex:outputText value="{0,number,###,###,##0.00}" rendered="{! IF(ISNULL(lineItems.IntPS_NSA__Not_Covered_Amount__c),false,true)}">
                                    <apex:param value="{!lineItems.IntPS_NSA__Not_Covered_Amount__c}" />
                                </apex:outputText>
                            </td>
                            <td style="text-align: center; padding-left: 5px;" class="slds-cell-wrap">
                                <apex:outputText value="{! IF(lineItems.IntPS_NSA__Service_Line_Allowed_Unit_Price__c==null,'No Rate',ROUND(lineItems.IntPS_NSA__Patient_Responsibility__c,2))}"> </apex:outputText>
                            </td>
                            <td style="text-align: center; padding-left: 5px;" class="slds-cell-wrap">
                                
                                <apex:outputText value="{! IF(ISNULL(lineItems.Remark_Code__c),lineItems.IntPS_NSA__Health_Plan_Item__r.Remark_Code__r.IntPS__Code__c,lineItems.Remark_Code__r.IntPS__Code__c)}"> </apex:outputText>
                            </td>
                            <td style="text-align: center; padding-left: 5px; border-left: 0.5px solid black;" class="slds-cell-wrap">
                                <apex:outputText value="{!lineItems.Provider_Type__c}" escape="false" />
                            </td>
                            <td style="text-align: center; padding-left: 5px; border-left: 0.5px solid black;" class="slds-cell-wrap">
                                <apex:outputText value="{!lineItems.PreventativePregn__c}" escape="false" />
                            </td>
                            <td style="text-align: center; padding-left: 5px; border-left: 0.5px solid black;" class="slds-cell-wrap">
                                <apex:outputText value="{!lineItems.IntPS_NSA__Place_Of_Service__c}" escape="false" />
                            </td>
                        </tr>
                    </apex:repeat>
                    <tr style="border-top: 0.5px solid black;">
                        <td class="slds-cell-wrap" style="height: 20px !important; background-color: #ffffff; border-top: 0.5px solid black;"></td>
                        <td colspan="2" class="slds-cell-wrap" style="height: 20px !important; background-color: #ffffff; border-top: 0.5px solid black;"></td>
                        <td class="slds-cell-wrap" style="height: 20px !important; background-color: #ffffff; border-top: 0.5px solid black;"></td>
