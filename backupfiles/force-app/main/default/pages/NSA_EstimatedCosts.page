                        <td class="slds-cell-wrap" style="height: 20px !important; background-color: #ffffff; border-top: 0.5px solid black;"></td>

                    </tr>
                    <tr style="border-top: 0.5px solid black;">
                        <td class="slds-cell-wrap" style="height: 20px !important; background-color: #ffffff; border-top: 0.5px solid black;"></td>
                        <td colspan="2" class="slds-cell-wrap" style="height: 20px !important; background-color: #ffffff; border-top: 0.5px solid black;"></td>
                        <td class="slds-cell-wrap" style="height: 20px !important; background-color: #ffffff; border-top: 0.5px solid black;"></td>
